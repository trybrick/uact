    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "raw-loader": "^0.5.1",
    "webpack": "^3.10.0",
    "wu": "github:niiknow/wu",
    "yargs": "^10.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trybrick/uact.git"
  },
  "keywords": [
    "webpack",
